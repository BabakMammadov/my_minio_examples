{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": ["s3:ListBucket","s3:GetBucketLocation","s3:ListAllMyBuckets"],
        "Resource": ["arn:aws:s3:::bucket_name"]
      },
      {
        "Effect": "Allow",
        "Action": ["s3:PutObject","s3:GetObject","s3:DeleteObject","s3:AbortMultipartUpload","s3:ListMultipartUploadParts"],
        "Resource": ["arn:aws:s3:::bucket_name/env_name/*"]
      }
    ]
  }
